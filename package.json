{
  "name": "via-appia-museum",
  "version": "1.0.0",
  "platform": "win x64",
  "main": "build/main.js",
  "repository": "git@github.com:Via-Appia/via-appia-museum.git",
  "author": "Jorne Envy <jornejongsma@gmail.com>",
  "license": "MIT",
  "company": "ProductBuilder",
  "productName": "Via Appia Museum App",
  "description": "Via Appia Museum App for Het Valkhof Nijmegen",
  "scripts": {
    "start:render": "webpack serve --config-name renderConfig --mode development",
    "start:main": "webpack --config-name mainConfig --mode development --watch",
    "start:electron": "nodemon --exec electron build/main.js APP_DEV",
    "start": "npm-run-all -l -p start:*",
    "build": "webpack --mode production",
    "dist": "yarn build && electron-builder build --win --publish never",
    "deploy": "yarn build && electron-builder build --win --publish always",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}"
  },
  "dependencies": {
    "@cognite/potree-core": "^1.1.6",
    "@tweenjs/tween.js": "^18.6.4",
    "dat.gui": "^0.7.7",
    "dayjs": "^1.10.4",
    "electron-updater": "^4.3.9",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "three": "^0.128.0",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "css-loader": "^5.2.5",
    "electron": "^10.1.5",
    "electron-builder": "^22.10.5",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "1.7.0",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "yarn-run-all": "^3.1.1"
  }
}
